QPushButton {
	background-color: rgb(96, 186, 255);
	border-radius:5px;
	margin: 5% 5%;
	padding: 10% 0;
	color: #eeeeff;
}

QPushButton:hover {
	background-color: #5aaaff;
}

QPushButton:pressed {
	background-color: #4584c3;
}

QLineEdit {
	background-color: #fff;
	margin: 0% 5%;
	padding: 7% 0;
	border-radius:5px;
	border: 1px solid grey;
}
